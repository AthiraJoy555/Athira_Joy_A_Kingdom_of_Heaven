Sql Code
IF NOT EXISTS (SELECT * FROM sysobjects WHERE name='booking_form' AND xtype='U')
BEGIN
    CREATE TABLE booking_form (
        id INT IDENTITY(1,1) PRIMARY KEY,
        full_name NVARCHAR(100) NOT NULL,
        address NVARCHAR(255),
        phone_number NVARCHAR(20),
        age INT,
        gender NVARCHAR(10),
        submitted_at DATETIME DEFAULT GETDATE()
    )
END

Flask 



from flask import Flask, render_template, request, redirect, flash
import sqlite3
from datetime import datetime

app = Flask(__name__)
app.secret_key = 'your_secret_key'  # Required for flash messages

# Database setup (SQLite)
def init_db():
    with sqlite3.connect('database.db') as conn:
        cursor = conn.cursor()
        cursor.execute('''
            CREATE TABLE IF NOT EXISTS booking_form (
                id INTEGER PRIMARY KEY AUTOINCREMENT,
                full_name TEXT NOT NULL,
                address TEXT,
                phone_number TEXT,
                age INTEGER,
                gender TEXT,
                submitted_at DATETIME DEFAULT CURRENT_TIMESTAMP
            )
        ''')
        conn.commit()

@app.route('/', methods=['GET', 'POST'])
def form():
    if request.method == 'POST':
        full_name = request.form['full_name']
        address = request.form['address']
        phone = request.form['phone']
        age = request.form['age']
        gender = request.form.get('gender')

        # Save to DB
        with sqlite3.connect('database.db') as conn:
            cursor = conn.cursor()
            cursor.execute('''
                INSERT INTO booking_form (full_name, address, phone_number, age, gender)
                VALUES (?, ?, ?, ?, ?)
            ''', (full_name, address, phone, age, gender))
            conn.commit()

        flash('Booking submitted successfully!')
        return redirect('/')

    return render_template('form.html')

if __name__ == '__main__':
    init_db()
    app.run(debug=True)
